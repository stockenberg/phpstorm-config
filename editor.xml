<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_SCALE" value="1.0833334" />
    <option name="LINE_SPACING" value="1.4" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bbug\b.*">
      <option name="FOREGROUND" value="ff3a45" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bnotice\b.*">
      <option name="FOREGROUND" value="3fccff" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>